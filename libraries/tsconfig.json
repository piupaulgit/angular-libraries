{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "circle-progress": [
        "dist/circle-progress/circle-progress",
        "dist/circle-progress"
      ],
      "ng-easy-progress-bar": [
        "dist/ng-easy-progress-bar/ng-easy-progress-bar",
        "dist/ng-easy-progress-bar"
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}